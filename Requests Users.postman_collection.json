		{
			"name": "Get user",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "11c6d2f8-4988-4747-ad4e-40c88526990b",
						"exec": [
							"let jsonData = pm.response.json();",
							"",
							"pm.test(\"All fields from request are equal to all fields from response\", function () {",
							"    pm.expect(jsonData.id).to.eql(34895);",
							"    pm.expect(jsonData.firstName).to.eql(\"Alex\");",
							"    pm.expect(jsonData.email).to.eql(\"a.storm@gmail.com\");",
							"    pm.expect(jsonData.password).to.eql(\"12309\");",
							"    pm.expect(jsonData.userStatus).to.eql(0);",
							"    ",
							"});",
							"",
							"pm.test(\"user_name from env is equal to username from response\", function () {",
							"    pm.expect(pm.environment.get(\"user_name\")).to.eql(\"alex123\");",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
